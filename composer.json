{
    "name": "rodymol123/flarum-ext-dutch",
    "type": "flarum-extension",
    "description": "Dutch language pack.",
    "homepage": "https://github.com/rodymol123/flarum-ext-dutch",
    "keywords": ["locale"],
    "license": "MIT",
    "require": {
        "flarum/core": "^0.1.0-beta.5"
    },
    "extra": {
        "flarum-extension": {
            "title": "Dutch",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#fff",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "nl",
            "title": "Nederlands"
        }
    }
}
